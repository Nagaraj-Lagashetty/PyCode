#strongPasssword
import re
while True:
    Password = input('Enter strong password again')
    if re.match(r'(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[@#$!])[a-zA-Z0-9@#$!)]{8}',Password):
        print('Thank you, your password is :', Password)
        break
    else:
        print('Enter strong password')

#Positive look ahead concept